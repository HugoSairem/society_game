{% extends 'admin-base.html.twig' %}
{% block content %}
    {% include 'admin-map-base.html.twig' %}
    <div class="admin-content">

        <h1>Users</h1>

        {% for user in users %}
            <ul>
                <li>{{ user.username }} | {{ user.email }} |
                    {% if user.enabled %}
                        Activated
                    {% endif %}
                    | {{ user.lastLogin |date }}
                </li>
            </ul>
        {% endfor %}
    </div>
{% endblock %}
