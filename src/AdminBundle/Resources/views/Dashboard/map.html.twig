{% extends 'admin-base.html.twig' %}
{% block content %}
{% include 'admin-map-base.html.twig' %}
<div class="admin-content">

    <h1>Map</h1>

    {% for solar in solars %}
        <ul class="solar-list">
            <li>Solar System: <b>{{ solar.name }}</b> <a class="delete float-right" href="{{ path('admin_dashboard_delete_solar',{'idSelectedSolar':solar.id}) }}"><span>Delete</span></a>
                <ul>
                    Planet :
                {% for planet in solar.planet %}
                    <a href="{{ path('admin_dashboard_map_overview',{'idSelectedPlanet':planet.id}) }}"><i> {{ planet.name }} </i></a>| </li>
                {% endfor %}
                </ul>
            </li>
        </ul>
    {% endfor %}
</div>
{% endblock %}
